@font-face
  font-family 'Nickainley'
  src url('./fonts/nickainley.woff') format('woff'),
      url('./fonts/nickainley.ttf') format('truetype'),
      url('./fonts/nickainley.woff2') format('woff2')
  font-weight 400
  font-style normal

@font-face
  font-family 'Lato'
  src url('./fonts/lato.woff') format('woff'),
